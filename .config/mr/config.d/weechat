[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/weechat.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-weechat.git conky
setpush  = vcsh weechat remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-conky.git
           vcsh weechat remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-conky.git
           vcsh weechat remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-conky.git

